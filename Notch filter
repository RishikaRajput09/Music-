b=[0.839,-1.55262,0.839] 
a=[1,-1.55262,0.64964] 
t=0:0.001:0.2 
x=cos(2*pi*60*t) 
plot(t,x) 
subplot(3,1,1),plot(t,x) 
y=filter(b,a,x) 
subplot(3,1,3),plot(t,y) 
w=0:0.01:pi 
h=freqz(b,a,w) 
subplot(3,1,2),plot(w,abs(h)) 


b=[0.839,-1.55262,0.839] 
a=[1,-1.55262,0.64964]  
t=0:0.001:0.2 
x=cos(2*pi*60*t)+cos(2*pi*100*t) 
plot(t,x) 
subplot(3,1,1),plot(t,x) 
y=filter(b,a,x) 
subplot(3,1,3),plot(t,y) 
w=0:0.01:pi 
h=freqz(b,a,w) 
subplot(3,1,2),plot(w,abs(h)) 
